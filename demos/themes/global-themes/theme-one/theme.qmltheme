/*

    The theme redefines the .Button and .button colors as well as
    defines delegate for .frame
*/

@import url("../default.qmltheme");
@qml-import(themes 1.0, current:/demos);

.button {
    color: item.hovered ? "tan" : "lightgray";
}

.custom-button {
    color: item.hovered ? "lightgray" : "#a3e5e8";
}

.button#custom-name {
    color: item.hovered ? "brown" : "tan"
}


.frame {
    color: "tan";
}

